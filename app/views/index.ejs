	                    <section ng-view></section>
	                </div>
	            </div>
	        </div>
	    </div>
	    <!-- /#page-content-wrapper -->

	</div>
	
	<!-- footer -->
	</nav>
	<script type="text/javascript">
    window.user = <%- user || 'null' %>;
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
